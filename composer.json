{
    "name": "qlimix/http-cors-middleware",
    "type": "library",
    "description": "PSR15 CORS middleware implementation",
    "license": "MIT",
    "keywords": [
        "cors",
        "middleware",
        "PSR15"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "neomerx/cors-psr7": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\HttpMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\HttpMiddleware\\": "tests/"
        }
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    },
    "config": {
        "sort-packages": true
    }
}
